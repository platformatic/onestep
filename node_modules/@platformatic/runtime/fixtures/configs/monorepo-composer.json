{
  "$schema": "https://platformatic.dev/schemas/v0.20.0/runtime",
  "entrypoint": "composerApp",
  "hotReload": false,
  "autoload": {
    "path": "../monorepo",
    "exclude": ["docs"],
    "mappings": {
      "serviceAppWithLogger": {
        "id": "with-logger",
        "config": "platformatic.service.json"
      },
      "serviceAppWithMultiplePlugins": {
        "id": "multi-plugin-service",
        "config": "platformatic.service.json"
      }
    }
  }
}
